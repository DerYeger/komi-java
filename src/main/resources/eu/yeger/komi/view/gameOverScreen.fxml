<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="40.0" style="-fx-background-color: #888888;" stylesheets="@main.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.yeger.komi.controller.GameOverScreenController">
   <children>
      <Label id="title-label" fx:id="winnerLabel" text="PLAYER has won!" />
      <Button fx:id="newGameButton" mnemonicParsing="false" text="New Game" />
      <Button fx:id="mainMenuButton" mnemonicParsing="false" text="Main Menu" />
      <Button fx:id="exitButton" mnemonicParsing="false" text="Exit" />
   </children>
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
</VBox>
