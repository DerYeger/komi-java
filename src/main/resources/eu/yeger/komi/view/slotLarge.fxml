<AnchorPane fx:id="anchorPane" prefHeight="70.0" prefWidth="70.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.yeger.komi.controller.SlotController">
   <children>
      <Line fx:id="topLine" endX="35.0" endY="35.0" startX="35.0" strokeWidth="2.0" visible="false" />
